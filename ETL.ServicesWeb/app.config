<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="ReportContract">
                    <security mode="Transport" />
                </binding>
                <binding name="ReportContract1" />
                <binding name="AuthorizationContract">
                    <security mode="Transport" />
                </binding>
                <binding name="AuthorizationContract1" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="https://ppws.astus.com/2010/11/ReportService.asmx"
                binding="basicHttpBinding" bindingConfiguration="ReportContract"
                contract="Report.ReportContract" name="ReportContract" />
            <endpoint address="https://ppws.astus.com/2010/11/AuthenticationService.asmx"
                binding="basicHttpBinding" bindingConfiguration="AuthorizationContract"
                contract="Authentification.AuthorizationContract" name="AuthorizationContract" />
        </client>
    </system.serviceModel>
</configuration>